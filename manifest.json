{
	"name": "Chatlink",
    "version": "2.1.1",
	"description": "View links for Hangouts Chat threads.",
	"browser_action": {},
	"permissions": ["activeTab", "webRequest", "https://chat.google.com/*", "storage"],
	"background": {
		"scripts": ["./scripts/background.js"],
		"persistent": true
	},
	"icons": {
	    "16": "icon16.png",
		"32": "icon32.png",
		"48": "icon48.png",
		"128": "icon128.png"
	},
	"manifest_version": 2
}